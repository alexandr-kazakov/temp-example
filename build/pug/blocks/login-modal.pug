          //- +btn("link", "#", "Facebook", iconFacebook)._theme_dark._icon._facebook
        +e.col_text-center.col-xl-4.col-lg-4.col-md-4.margin-b-xs
          +btn("link", "#", "Вконтакте", iconVK)._theme_dark._icon._vk
        +e.col_text-left.col-xl-4.col-lg-4.col-md-4
