- var clockTwo= '<svg class="restaurant-card__icon active" xmlns="http://www.w3.org/2000/svg" '

mixin restaurant-card(box) 
  // begin Restaurant-card
  +b.A.restaurant-card(href="#")&attributes(attributes)
    +e.DIV.bg-image-wrapper
      +e.bg-image(style="background-image: url(" + box.imgBG + ")")
    +e.DIV.content
      +e.logo
        +e.IMG.logo-image(src= box.imgLogo.imgLogoSrc , alt= box.imgLogo.imgLogAlt)
      +e.H3.title= box.title
      +e.SPAN.kitchen= box.kitchen
      +restaurant-data-box({minPrice: box.minPrice}, {maxTime: box.time })._sm.justify-c-c 

    +e.DIV.footer
      +stars-rating({stars: box.stars})._lg._margin_r_md
      +e.SPAN.reviews= box.reviews + " отзывов"
  // end Restaurant-card