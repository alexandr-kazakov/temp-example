mixin social-block(box)
  // begin Social-block
  +b.social-block&attributes(attributes)
    +e.UL.items
      +e.LI.item
        +e.A.item-link(href="https://vk.com/eatsfast" target="_blank")._vk 
      +e.LI.item
        +e.A.item-link(href="https://www.facebook.com/eatsfast.ru" target="_blank")._facebook 
      +e.LI.item
        +e.A.item-link(href="https://www.instagram.com/eats.fast/" target="_blank")._instagram  
  // end Social-block